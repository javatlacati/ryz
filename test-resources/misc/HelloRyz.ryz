hello.HelloRyz{
  main( args: String* ) {

    // Sort descending
    args.sort!( (a:String, b.String ):Int {
      b .<=> a
    })

    // iterate the array and greet
    // the value in arg
    args.each((value:String){
      out.print "Hello %s".% value 
    })
  }
}
